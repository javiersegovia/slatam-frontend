{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true
      }
    ],
    ["@babel/plugin-proposal-class-properties"],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@config": "./config",
          "@components": "./src/components",
          "@graphql": "./graphql",
          "@lib": "./src/lib",
          "@hooks": "./src/hooks",
          "@pages": "./pages",
          "@root": "./",
          "@views": "./src/views"
        }
      }
    ]
  ]
}